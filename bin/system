#!/usr/local/bin/php
<?php

for (
    $_prjdir = dirname($_composer_autoload_path ?? __FILE__);
    strlen($_prjdir) > 1 && file_exists("$_prjdir/composer.json") === false;
    $_prjdir = dirname($_prjdir)
);
include $_composer_autoload_path ?? "$_prjdir/vendor/autoload.php";
(new davekok\system\System($_prjdir))->boot($argv)->run();
